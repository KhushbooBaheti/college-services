<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.himanshu.canteen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/college"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:screenOrientation="portrait"/>
        <activity android:name=".LoginActivity" android:screenOrientation="portrait"/>
        <activity android:name=".SignUp" android:screenOrientation="portrait"/>
        <activity android:name=".Startup" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MerchantPage"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".MenuPage" android:screenOrientation="portrait"
            android:label="Amul Shop"/>
        <activity android:name=".BillPage" android:screenOrientation="portrait"
            android:label="Order Bill"/>
    </application>

</manifest>